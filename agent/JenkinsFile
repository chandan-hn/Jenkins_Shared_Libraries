pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'ls'
                sh 'sleep 10'
            }
        }

        stage('Test1') {
            steps {
                sh '''
                    #!/bin/bash
                    ls -ltr
                    echo "------------Test 1 stage------------------"
                    'sleep 10'
                '''
                
            }
        }

        stage('Test2') {
            steps {
                sh '''
                    #!/bin/bash
                    ps -ef
                    echo "------------Test 2 stage------------------"
                    'sleep 10'
                '''
            }
        }
    }
}
